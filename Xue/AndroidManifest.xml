<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MeadowEast.xue"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.MeadowEast.xue.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_main" >
>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.MeadowEast.xue.LearnActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_CEActivity" >
>
        </activity>
        <activity
            android:name="com.MeadowEast.xue.CardUpdater"
            android:excludeFromRecents="true"
            android:noHistory="true"
            android:theme="@android:style/Theme.Dialog" >
>
        </activity>
        <activity
            android:name="com.MeadowEast.xue.LogActivity"
            android:label="@string/title_activity_log"
            android:parentActivityName="com.MeadowEast.xue.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.MeadowEast.xue.MainActivity" />
        </activity>
    </application>

</manifest>